    
   <div class="sidenav" >
     <div class="user" id="user" style="z-index: 22;" onclick="run(event)">
       <i class="fa fa-user" ></i>
     </div>
     {{#if login}}
     <div class="user profile" id="profile">
       <a href="/user/profile"><i class="fa fa-user"></i></a>
     </div>
     <div class="user logout" id="logout">
       <a href="/user/logout"><i class="fa fa-sign-out" aria-hidden="true"></i></a>
     </div>
     {{ else }}
     <div class="user signin" id="signin">
       <a href="/user/signin" ><i class="fa fa-sign-in" aria-hidden="true"></i></a>
     </div>
     <div class="user signup" id="signup">
       <a href="/user/signup"><i class="fa fa-user-plus" aria-hidden="true"></i></a>
     </div>
     {{/if}} 

       <img onclick="location.href='/';" src="https://img.apksum.com/7d/com.bazaar.app/2.1.5.6/icon.png" alt="">         
     
     <div class="shop">
       <a href="/shopping-cart/"><img src="https://www.pinclipart.com/picdir/big/179-1792672_shopping-clip-art-black-and-white.png" alt=""><span class="badge">{{session.cart.totalQty}}</span></a>
     </div>
   </div>

     
   